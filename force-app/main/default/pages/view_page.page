<apex:page standardController="Student__c">
    <apex:form >
        <apex:sectionHeader title="View Detail" subtitle="Student" />    
        <apex:pageBlock title="Student View PageBlock">
            
            <apex:pageBlockSection > 
                <apex:outputField value="{!Student__c.Name}"/>
                <apex:outputField value="{!Student__c.Email_Id__c}"/>
                <apex:outputField value="{!Student__c.Gender__c}"/>
                <apex:outputField value="{!Student__c.Mobile_No__c}"/>
                <apex:outputField value="{!Student__c.Date_And_Time__c}"/>  
            </apex:pageBlockSection>
            
            <apex:pageBlockButtons >
                <apex:commandButton action="{!cancel}" value="cancel"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>